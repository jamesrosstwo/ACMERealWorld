{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Collect",
      "type": "debugpy",
      "request": "launch",
      "module": "client.collect",
      "args": ["task=${fileBasenameNoExtension}"],
      "python": "${config:python.defaultInterpreterPath}",
      "cwd": "${workspaceFolder}",
      "justMyCode": true
    },
    {
      "name": "Eval",
      "type": "debugpy",
      "request": "launch",
      "module": "client.eval",
      "args": ["task=${fileBasenameNoExtension}"],
      "python": "${config:python.defaultInterpreterPath}",
      "cwd": "${workspaceFolder}",
      "justMyCode": true
    },
    {
      "name": "Postprocess",
      "type": "debugpy",
      "request": "launch",
      "module": "client.postprocess",
      "python": "${config:python.defaultInterpreterPath}",
      "cwd": "${workspaceFolder}",
      "justMyCode": true
    }
  ]
}
